    "tailwindcss": "^4",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.26.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.8",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
