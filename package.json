{
  "homepage": "https://simone.erba.github.io/dnd-random-spell-generator",
  "name": "react-next-boilerplate",
  "version": "1.2.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"src/**/*.+(ts|tsx)\"",
